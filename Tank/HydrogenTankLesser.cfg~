PART
{
	name = Fumes_Hydrogen_Tank_Lesser
	module = Part
	author = Ed Gauss
	mesh = Radial_Tank.mu
	rescaleFactor = 0.5
	node_attach = 0.0, 0.0, 0, -1, 0, 0
	TechRequired = start
	entryCost = 5400
	cost = 225
	category = FuelTank
	subcategory = 0
	title = FUMES Hydrogen Tank, Lesser
	description = A simple tank containing hydrogen gas.
	attachRules = 1,1,0,0,1
	mass = 0.3578 //Type IV tank: 0.824 kg/L (28 kg/34 L)
	dragModelType = default
	maximum_drag = 0.2
	minimum_drag = 0.2
	angularDrag = 2
	crashTolerance = 12
	maxTemp = 2000 // = 2900
	bulkheadProfiles = srf
	RESOURCE
	{
		//Tank volume: 434.25 L (Attachment pieces hold nothing)
		//The type IV tank can hold hydrogen at 661 bars,
		//which is 652.36 times standard pressure 
		//(101.325 kPa = 1.01325 bar).
		//This means this tank will hold 652.36 times it's
		//volume in hydrogen gas.
		//434.25 L * 652.36 = 283287.33 L
		name = Hydrogen
		amount = 283287
		maxAmount = 283287
	}
}
